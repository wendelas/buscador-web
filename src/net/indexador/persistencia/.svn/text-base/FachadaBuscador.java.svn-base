package net.indexador.persistencia;

import org.apache.log4j.Logger;

public class FabricaBuscador {
  private static final Logger logger = Logger.getLogger(FabricaBuscador.class);
  private static FabricaBuscador instancia;

  public FabricaBuscador() {
  }

  /**
   * getInstancia
   */
  public static synchronized FabricaBuscador getInstancia()
      throws FabricaException {
    if (instancia == null) {
      instancia = new FabricaBuscador();
    }
    return instancia;
  }
}
