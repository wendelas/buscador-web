<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

	<application>
		<message-bundle>MessageResources</message-bundle>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
		<locale-config>
			<default-locale>pt_BR</default-locale>
		</locale-config>
		<message-bundle>net.visualizacao.apresentacao.mensagens</message-bundle>
	</application>

	<managed-bean>
		<managed-bean-name>frequenciaDosTermosBean</managed-bean-name>
		<managed-bean-class>net.visualizacao.apresentacao.FrequenciaDosTermosBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>fonteDadosBean</managed-bean-name>
		<managed-bean-class>net.visualizacao.apresentacao.FonteDadosBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>buscaLivreBean</managed-bean-name>
		<managed-bean-class>net.visualizacao.apresentacao.BuscaLivreBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>utilitarioBean</managed-bean-name>
		<managed-bean-class>net.visualizacao.apresentacao.UtilitarioBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>baseBean</managed-bean-name>
		<managed-bean-class>net.visualizacao.apresentacao.BaseBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<from-view-id>/paginas/FonteDados.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/paginas/Utilitario.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancelar</from-outcome>
			<to-view-id>/paginas/Utilitario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/paginas/Utilitario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editar</from-outcome>
			<to-view-id>/paginas/FonteDados.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>